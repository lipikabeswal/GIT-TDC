									}else{
										gFcatFormulaCard.setAttribute("isselected",true);
										gFcatFormulaCard.setAttribute('x',(this.width/2-gFcatFormulaCard.width/2));
										gFcatFormulaCard.setAttribute('y',(this.height/2-gFcatFormulaCard.height/2));
										gFcatFormulaCard.show();
