		<!--added for tdc timeout OK start-->

		<attribute name="trackDel" 
                   value="$once{new LzDelegate( this , 'trackMouseMove' ) }" />

		<method name="trackMouseMove" >
            <![CDATA[
            var xpos = Math.round( canvas.getMouse("x") );
            var ypos = Math.round( canvas.getMouse("y") );
            if ( (this.lastxpos != xpos) 
              || (this.lastypos != ypos) ) {
                this.lastxpos = xpos;
                this.lastypos = ypos;
				this.setAttribute('trackTimer',0);
                resetTrackTimer();
            }else{
            	this.setAttribute('trackTimer',Math.round(this.trackTimer+1000));
            	if(this.trackTimer >= timerInterval){ 
					gCommunicator.closeOkCalculator();           		
					stopQuestion();
					removeTrackTimer();
					gScreens.showTimeoutForInactivity();
					canvas.setPromptDimension();
            		timeoutDialog.open();					
            	} else {
					resetTrackTimer();
				}
            }
            ]]>
        </method>
        
        <method name="resetTrackTimer">
			//Debug.write("trackTimer",this.trackTimer);
			LzTimer.resetTimer(trackDel,1000);
