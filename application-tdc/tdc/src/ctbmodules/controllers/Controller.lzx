		<method name="getCurrentSubtestParams">
			var currentSubtest = getCurrentSubtest();
			var id = currentSubtest.getAttr('adsid');
			var hash = currentSubtest.getAttr('asmt_hash');
			var key = currentSubtest.getAttr('asmt_encryption_key');
