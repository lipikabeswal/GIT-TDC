		<method event="oncommunicatorResponse" args="response">
			var responseState = state;
			if(state != DOWNLOAD_STATE){
				setState(NONE_STATE);
			}
			if(_isCommunicationError(response)){
				_handleError(response);
			}
			else {
				switch (responseState){
					case LOGIN_STATE:
						_handleLoginResponse(response);
						break;
					case DOWNLOAD_STATE:
						_handleDownloadResponse(response);
						break;
					case SUBTEST_STATE:
						_handleSubtestResponse(response);
						break;
					case SAVE_LOAD_STATE:	
						_handleSaveLoadResponse(response);
						break;
					case PAUSE_STATE:	
						_handlePauseResponse(response);
						break;
					case PAUSED_RESUME_STATE:	
						_handlePausedResumeResponse(response);
						break;
					case TIMEOUT_STATE:
						_handleTimeoutResponse(response);
						break;
					case FINISH_STATE:
						_handleFinishResponse(response);
						break;
					case WRITE_TO_AUDIT_FILE_STATE:
						_handleWriteToAuditFileResponse(response);
						break;
					case UPLOAD_AUDIT_FILE_STATE:
						_handleUploadAuditFileResponse(response);
						break;
					case FINISH_SAVE_STATE:
						_handleFinishSaveResponse(response);
						break;
					case SAVE_END_SUBTEST_STATE:
						_handleSaveEndSubtestResponse(response);
						break;
					case SUBTEST_FINISH_STATE:
						_handleSubtestFinishResponse(response);
						break;
					case OUT_OF_TIME_OPEN_STATE:
						_handleOutOfTimeOpenResponse(response);
						break;
					case FEEDBACK_STATE:
						_handleFeedbackResponse(response);
						break;
					case SCORES_STATE:
						_handleScoresResponse(response);
						break;
					case TUTORIAL_COMPLETE_STATE:
						_handleTutorialCompleteResponse(response);
						break;
					case CALCULATOR_CLOSE:
						_handleCloseCalculatorResponse(response);
						break;
					case OK_CALCULATOR_STATE:
						_handleOkCalculatorResponse(response);
						break;
					default:
						break;
				}
			}
		</method>

		<method name="_handleUploadAuditFileResponse" args="response"><![CDATA[
			var rawscore = 0;
			var visited = 0;
			var viewedCount = 0;
            // clock is stopped
            var remainingms = durationms - elapsedms;
            if ( remainingms < 0 ) remainingms = 0;
            var remainingsecs = (remainingms / 1000);
            var remainingmins = remainingsecs / 60;
            var remaininghours = remainingmins / 60;

            var secs = Math.floor( remainingsecs ) % 60;
            var mins = Math.floor( remainingmins ) % 60;
            var hours = Math.floor( remaininghours );

            if ( secs < 10 ) secs = '0' + secs;
            if ( mins < 10 ) mins = '0' + secs;
            if ( hours < 10 ) hours = '0' + secs;

            // sum of correct answers
            if(this.getAttribute('isOK') == 'T') { //For Oklahoma Customer
				if(dprestart.isValid()){ // for restart scenario
					var restartData = dprestart.xpathQuery('tsd/ist');
					var itemIds = new Array();
					var itemScores = new Array();
					if(restartData && restartData instanceof Array) {
						for(var i=0; i<restartData.length; i++) {
							var visitedItemNode = restartData[i];
							var visitedItemId = visitedItemNode.getAttr('iid');
							itemIds.push(visitedItemId); 
							if(visitedItemId.indexOf('FT') > 0) {
								itemScores.push(0);
			         			continue;
			         		}
			         		else {
								var xPath = "tsd/ist[@iid='" + visitedItemId + "']";
			         			var raw = dprestart.xpathQuery(xPath + "/ov[@n='SCORE']/v/text()");
								if(raw != null && raw != undefined && raw != "") {
									itemScores.push(raw);
				         			rawscore = Number(rawscore) + Number(raw);
								} else {
									// It means this might a NI or theme item
									itemScores.push(0);
								}
			         		}
						}
					}
					else if(restartData && restartData instanceof LzDataElement) {
			        	var visitedItemId = restartData.getAttr('iid');
						itemIds.push(visitedItemId);
						var xPath = "tsd/ist[@iid='" + visitedItemId + "']";
		         		if(itemId.indexOf('FT') <= 0) {
		         			var raw = dprestart.xpathQuery(xPath + "/ov[@n='SCORE']/v/text()");
							if(raw != null && raw != undefined && raw != "") {
								itemScores.push(raw);
			         			rawscore = Number(rawscore) + Number(raw);
							} else {
								// It means this might a NI or theme item
								itemScores.push(0);
							}
		         		}
						else {
							itemScores.push(0);
						}
		        	}
					var isVisited = "false";
					var viewedItems = dpstudentdata.xpathQuery("runtime/item_model[@is_visited='true']"); 
					if (viewedItems && viewedItems instanceof Array ) {
			         	for(var i=0; i<viewedItems.length; i++) {
							isVisited = "false";
			         		var itemNode = viewedItems[i];
			         		var itemId = itemNode.getAttr('iid');
							var itemScore = itemNode.getAttr('score');

							if(itemScore == "" || itemScore == null || itemScore == undefined) {
								itemScore = 0;
							}	
							for(var j=0; j<itemIds.length; j++) {
								var visitedId = itemIds[j];
								var visistedIdScore = itemScores[j];
								if(visitedId == itemId) {
									if(visistedIdScore != itemScore) {
										if(itemId.indexOf('FT')<= 0) {
											rawscore = Number(rawscore) - Number(visistedIdScore) + Number(itemScore);
										}
									}
									isVisited="true";
									break;
								}
							}	
							if(isVisited == "false") {
								viewedCount++;
								if(itemId.indexOf('FT')<= 0) {
		         					var raw = itemNode.getAttr('score');
		         					//for non SR items, the raw score will be null, so should be set to zero to avoid "NaN" issues
		         					if(raw == "" || raw == null || raw == undefined) {
		         						raw = 0;
		         					}
		         					rawscore = Number(rawscore) + Number(raw);

									//Debug.write("rawscore 22.......", rawscore);
		         				}
							}
			         	}
			        }
			        else if(viewedItems && viewedItems instanceof LzDataElement) {
			        	var itemId = viewedItems.getAttr('iid');
						var itemScore = viewedItems.getAttr('score');
						if(itemScore == "" || itemScore == null || itemScore == undefined) 
							itemScore = 0;
						for(var i=0; i<itemIds.length; i++) {
							var visitedId = itemIds[i];
							var visistedIdScore = itemScores[i];
							if(visitedId == itemId) {
								if(visistedIdScore != itemScore) {
									if(itemId.indexOf('FT')<= 0) {
										rawscore = Number(rawscore) - Number(visistedIdScore) + Number(itemScore);
									}
								}
								isVisited="true";
								break;
							}
						}
						if(isVisited == "false") {
							if(itemId.indexOf('FT') <= 0) {
	         					var raw = viewedItems.getAttr('score');
	         					rawscore = Number(rawscore) + Number(raw);
	         				}
						}
					}
				}
				else { 
	            	 var viewedItems = dpstudentdata.xpathQuery("runtime/item_model[@score='1']");   
			         if ( viewedItems && viewedItems instanceof Array ) {
			         	for(var i=0; i<viewedItems.length; i++) {
			         		var itemNode = viewedItems[i];
			         		var itemId = itemNode.getAttr('iid');
			         		if(itemId.indexOf('FT') > 0) {
			         			continue;
			         		}
			         		else {
			         			var raw = itemNode.getAttr('score');
			         			rawscore = Number(rawscore) + Number(raw);
			         		}
			         	}
			        }
			        else if(viewedItems && viewedItems instanceof LzDataElement) {
			        	var itemId = viewedItems.getAttr('iid');
		         		if(itemId.indexOf('FT') <= 0) {
		         			var raw = viewedItems.getAttr('score');
		         			rawscore = Number(rawscore) + Number(raw);
		         		}
			        }
	            }
			} 
            else { //for non OK customers
	            rawscore = countDataNodes(
	                dpstudentdata.xpathQuery(
	                    "runtime/item_model[@score='1']"
	                )
	            );
            }
            gController.setAttribute('totalRaw',rawscore);

            var crtotal = countDataNodes(
                dpstudentdata.xpathQuery(
                    "runtime/item_model/interaction[@type='text_entry']"
                )
            );
			
            var grtotal = countDataNodes(
                dpstudentdata.xpathQuery(
                    "runtime/item_model/interaction[@type='grid']"
                )
            );
			
			var notSrOnRestart = countDataNodes(
                dpstudentdata.xpathQuery(
                    "runtime/item_model[@not_sr='t']"
                )
            );
			
            var maxpossible = totalQuestions();
            if(this.adaptive == 'true'){
            	maxpossible = gController.getAttribute("subtestLength");
            }
            
			//get the number of unscored items
			if(dprestart.isValid()){
				var visitedRestartCount = countDataNodes(
							dprestart.xpathQuery('tsd/ist')
					);
				visited = visitedRestartCount + viewedCount;
			}
			else {
				visited = countDataNodes(
		                dpstudentdata.xpathQuery(
		                    "runtime/item_model[@is_visited='true']"
		                )
		            );
			}
			var unscored = maxpossible - visited;
			
            var completionmode = ""; // empty string or 'time-out' - need to fix for timeout
            // time left in hh:mm:ss format
            var sessiontime = hours + ':' + mins + ':' + secs; 
            //var unscored = crtotal + grtotal + notSrOnRestart;

			//delete consolidated_restart_data node if there is one or no child tsd nodes 
			//deleteParentIfNoOrOneChild(dprestart);

			//START Change for deferred Defect 63502
			//dprestart.selectChild();
			//END
            //clear restart xml
            //dprestart.deleteNode();
			//START Change for deferred Defect 63502
			//dprestart.selectParent();
			//END

			if(this.adaptive == 'false'){
				gController.setAttribute("abScore", 0);
				gController.setAttribute("sem", 0);
				gController.setAttribute("objScore", null);
			}
			
			setState(FINISH_SAVE_STATE);
			gCommunicator.endSubtest( rawscore, maxpossible, completionmode, sessiontime, unscored );
		]]></method>

