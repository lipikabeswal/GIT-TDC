	                if(canvas.currentCursor == 'highlighter_cursor'){
	                	clearAndRestoreCurrentCursor();
	                }
					// to clear the eraser cursor when navigating to next item. 
					else if(canvas.currentCursor == 'eraser_cursor'){
	                	clearAndRestoreCurrentCursor();
	                }
					// to clear the eraser cursor when navigating to next item. 
					else if(canvas.currentCursor == 'eliminator_cursor'){
	                	clearAndRestoreCurrentCursor();
	                }
					else{	                	
	                	restoreCurrentCursor();
