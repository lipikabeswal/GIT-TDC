	<method name="addImageFields" args="field"><![CDATA[
		this.idImageFields.push(field);
		]]></method>

	<attribute name="dCheckPlaying"
			value="$once{ new LzDelegate( this, 'isPlayed' )}" />

	<method name="enableAudio"><![CDATA[
		sortByPlayorder();
		if(answerArray != null)
			enableAnswerOptions();
		if(crFields != null)
			enableResponseArea();
		
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			if(cachableImage['imageComponent'] instanceof CachableImage){
				if(cachableImage['imageComponent'].imageview.getMCRef().isPlaying && this.isAudioPlaying && !cachableImage['imageComponent'].hasPlayed){
					cachableImage['imageComponent'].widget_id = this.audioId;
					if(i==this.idImageFields.length-1){
						this.setAttribute("lastPlayOrderId",cachableImage['imageComponent'].widget_id);
					}
					cachableImage['imageComponent'].setAttribute('hasPlayed',true);

					this.idImageFieldTemp=cachableImage['imageComponent'];
					this.idImageFieldTemp.overlay.setAttribute("visible",true);
					this.idImageFieldTemp.overlay.setAttribute("clickable",true);

					if(this.recorder != null) {
						disableRecorder();
					}
					
					isPlayed();	
				}
				else if(cachableImage['imageComponent'].imageview.getMCRef().isPlaying && this.isAudioPlaying && cachableImage['imageComponent'].hasPlayed){
					this.idImageFieldTemp=cachableImage['imageComponent'];
					if(this.recorder != null) {
						disableRecorder();
					}
					isPlayed();
				}
				else if(!cachableImage['imageComponent'].imageview.getMCRef().isPlaying){
					cachableImage['imageComponent'].overlay.setAttribute("visible",true);
					cachableImage['imageComponent'].overlay.setAttribute("clickable",true);
				}
			}
		}
	]]>	
	</method>

	<method name="enableNextAudio" args="widgetId"><![CDATA[
		var contentArea = dpsubtestdata.getNodeText();
		var dncurritem = dpstudentdata.xpathQuery ('current_item/item_model');
	
		for(var i=0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			cachableImage['imageComponent'].overlay.setAttribute("visible",false);
			cachableImage['imageComponent'].overlay.setAttribute("clickable",false);

			if(cachableImage['imageComponent'].widget_id == widgetId){
				for(var j =i+1;j<this.idImageFields.length;j++){
					var cachableImageTemp = this.idImageFields[j];
					if(Number(cachableImage['imageComponent'].playorder) == Number(cachableImageTemp['imageComponent'].playorder)-1){
						if(cachableImage['imageComponent'].getAttribute('hasPlayed') && !cachableImageTemp['imageComponent'].getAttribute('hasPlayed')){
							if(j==this.idImageFields.length-1 
								&& (cachableImageTemp['imageComponent'].enableWhenAnswered == true || cachableImageTemp['imageComponent'].enableWhenAnswered == 'true')
								&& (contentArea == "Reading" || contentArea == 'Listening')){
								if(dncurritem.getAttr ('answered')==1){
									cachableImageTemp['imageComponent'].imageview.getMCRef().enableBtn();
								}
							}else{
								cachableImageTemp['imageComponent'].imageview.getMCRef().enableBtn();
							}
						}
					}
				}
			}
		}
		]]>
	</method>

	<method name="isPlayed">
		<![CDATA[
		if(!this.idImageFieldTemp.imageview.getMCRef().isPlaying){
			this.idImageFieldTemp.overlay.setAttribute("visible",false);
			this.idImageFieldTemp.overlay.setAttribute("clickable",false);
			if(this.recorder != null) {
				enableRecorder();
			}
			stopCheckPlaying();
		}else{
			resetCheckPlaying();
		}
	]]>	
	</method>

	<method name="resetCheckPlaying">
		LzTimer.resetTimer( dCheckPlaying, 1000 );
	</method>

	<method name="stopCheckPlaying"><![CDATA[
		--uisemaphore;
		LzTimer.removeTimer(dCheckPlaying);
		if(this.idImageFieldTemp.widget_id == this.lastPlayOrderId){
			unlockUI();
		}
		enableNextAudio(this.idImageFieldTemp.widget_id);
		this.idImageFieldTemp = null;		
	]]>
	</method>
	
	<method name="resetImageField"><![CDATA[
		 for (var i = 0; i < this.idImageFields.length; i++) {
			 var cachableImage = this.idImageFields[i];
			 cachableImage['imageComponent'].setAttribute("hasPlayed",false);
             delete this.idImageFields[i];
         }
         this.idImageFields.length = 0;
	]]></method>

	<method name="enableRecorder">
		if(this.recorder != null) {
			this.recorder.blackOverLay.setAttribute('visible',false);
		}
	</method>

	<method name="disableRecorder">
		if(this.recorder != null) {
			this.recorder.blackOverLay.setAttribute('visible',true);
		}
	</method>

	<method name="sortByPlayorder"><![CDATA[
		var temp;
		 for (var i = 0; i < this.idImageFields.length; i++) {
			 for (var j =1; j < this.idImageFields.length-i; j++) {
				var order = this.idImageFields[j-1];
				var orderTemp = this.idImageFields[j];

				if(order['imageComponent'].playorder > orderTemp['imageComponent'].playorder){
					temp=this.idImageFields[j-1];
					this.idImageFields[j-1]=this.idImageFields[j];
					this.idImageFields[j]=temp;
				}
			}
		}
	]]>
	</method>
	
	<method name="disableAllAudio"><![CDATA[	
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			if(cachableImage['imageComponent'] instanceof CachableImage){
				if(!cachableImage['imageComponent'].imageview.getMCRef().disabled){
					cachableImage['imageComponent'].imageview.getMCRef().disableBtn();
					this.audioArray.push(cachableImage['imageComponent'].widget_id);
				}
			}
		}
		]]>
	</method>

	<method name="enableAllAudio"><![CDATA[
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			for(var j=0; j<this.audioArray.length; j++){
				if(cachableImage['imageComponent'] instanceof CachableImage){
					if(this.audioArray[j]==cachableImage['imageComponent'].widget_id){
						cachableImage['imageComponent'].imageview.getMCRef().enableBtn();
					}
				}
			}
		}
		for(var k=0; k<this.audioArray.length; k++){
			delete this.audioArray[k];
		}
		this.audioArray.length=0
	]]>	
	</method>

	<method name="disableResponseArea"><![CDATA[
		var id = isAnswerToBeDisabled();
		if(id != null){
			for (var i = 0; i < this.crFields.length; i++){
				var crField = this.crFields[i];
				if(crField['ref'] instanceof CachableMultiLineAnswer){
					crField['ref'].outer.rich.answer.setAttribute('enabled',false);
				}
				if(crField['ref'] instanceof CachableSingleLineAnswer){
					crField['ref'].answer.disable();
				}
			}
		}
	]]>
	</method>
	
	<method name="enableResponseArea"><![CDATA[
		var id = isAnswerToBeDisabled();
		var isPlaying = checkIfPlaying(id);
		if(isPlaying){
			for (var i = 0; i < this.crFields.length; i++){
				var crField = this.crFields[i];
				if(crField['ref'] instanceof CachableMultiLineAnswer){
					crField['ref'].outer.rich.answer.setAttribute('enabled',true);
				}
				if(crField['ref'] instanceof CachableSingleLineAnswer){
					crField['ref'].answer.enable();
				}
			}
		}
	]]>
	</method>

	<method name="isAnswerToBeDisabled"><![CDATA[
		var id;
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			if(cachableImage['imageComponent'] instanceof CachableImage){
				if(cachableImage['imageComponent'].disableAnswerArea == true || cachableImage['imageComponent'].disableAnswerArea == 'true'){
					id = cachableImage['imageComponent'].widget_id;
					break;
				}
			}
		}
		return id;
	]]>
	</method>

	<method name="checkIfPlaying" args="id"><![CDATA[
		var isPlaying = false;
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			if(cachableImage['imageComponent'] instanceof CachableImage){
				if(cachableImage['imageComponent'].widget_id == id && cachableImage['imageComponent'].imageview.getMCRef().isPlaying){
					isPlaying = true;
					break;
				}
			}
		}
		return isPlaying;
	]]>
	</method>
	
	<method name="enableMoreInfo"><![CDATA[
		Debug.write("idImageFields********** ",this.idImageFields);
		var contentArea = dpsubtestdata.getNodeText();
		for(var i =0;i<this.idImageFields.length;i++){
			var cachableImage = this.idImageFields[i];
			if(cachableImage['imageComponent'] instanceof CachableImage){
				if(contentArea == "Reading" || contentArea == 'Listening'){
					if(i>0){
						var prevCachableImage = this.idImageFields[i-1];
					}
					
					if((cachableImage['imageComponent'].enableWhenAnswered == true || cachableImage['imageComponent'].enableWhenAnswered == 'true') && i==this.idImageFields.length-1 
						&& prevCachableImage['imageComponent'].hasPlayed && !prevCachableImage['imageComponent'].imageview.getMCRef().isPlaying){
						cachableImage['imageComponent'].imageview.getMCRef().enableBtn();
					}
				}
			}
		}
		]]>
	</method>

	</node>

