<?xml version="1.0" encoding="UTF-8"?>
<element_package iid="FCAT_TEST.002"
	security_classification_id="alg112">
	<item_canvas id="IC">
		<panel left_padding="12" stereotype="directions" id="stem1"
			width="500" layout="vertical" spacing="12" x="0" y="79"
			height="37">
			<text_widget halign="left" x="0" width="364"
				id="widget1000">
				<![CDATA[<B>Workstation Sound Check</B>]]>
			</text_widget>
		</panel>
		<scrolling_text_panel height="390" id="TD.6.0_010"
			stereotype="stimulus" width="360" x="12" y="105">
			<text_widget halign="left" width="350" id="widget1003"><![CDATA[<br/><B>Please go through the following checklist before running the workstation tuning utility:</B><br/>]]></text_widget>
			<text_widget halign="left" width="350" id="widget1004"><![CDATA[  -  Check to see that your microphone is connected]]></text_widget>
			<text_widget halign="left" width="350" id="widget1005"><![CDATA[  -  Check to ensure your microphone is not set to "Mute"]]></text_widget>
			<text_widget halign="left" width="350" id="widget1006"><![CDATA[  -  Set your microphone recording level to at least 70%; do not set the microphone to a low recording volume]]></text_widget>
			<text_widget halign="left" width="350" id="widget1007"><![CDATA[  -  Make sure the microphone is not in "Disabled" mode in your Control Panel]]></text_widget>
			<text_widget halign="left" width="350" id="widget1008"><![CDATA[  -  Make sure the speaker volume is turned up to an audible level]]></text_widget>
		</scrolling_text_panel>
		<panel stereotype="stem" id="stem3"
			layout="vertical" spacing="12" width="413" x="380" y="105"
			height="214">
			<image_widget autoenlargeable="yes" autoplay="true"
				enlargeable="no" halign="left" height="350"
				src="calibrationText.png" isaudio="false" playonce="false"
				playorder="1" width="395" isCalibration="true"/>
		</panel>
		<answer_area_panel id="A1" stereotype="answerArea" width="390"
			layout="vertical" spacing="12" x="410" height="180" top_padding="12"
			left_padding="12" y="330">
			<recorder_widget x="1" y="1"></recorder_widget>
		</answer_area_panel>
	</item_canvas>
	<item_model answer_text="" iid="FCAT_TEST.002" eid="0" marked="0"
		answered="0" number="0" type="question" correct="A">
		<toolbar oneeighth_inch_ruler="required" mm_ruler="required"
			formula_card="required" FCAT_formula_card="required"
